{
	"$schema": "https://biomejs.dev/schemas/1.9.0/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"lineWidth": 80
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "off"
			},
			"complexity": {
				"noForEach": "off"
			}
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single",
			"arrowParentheses": "always",
			"enabled": true,
			"indentWidth": 2,
			"indentStyle": "tab",
			"bracketSpacing": true,
			"lineWidth": 80,
			"semicolons": "asNeeded",
			"jsxQuoteStyle": "single"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		},
		"linter": {}
	}
}